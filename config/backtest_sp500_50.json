{
    "price_csv_path": "data/sp500_50_prices.csv",
    "return_type": "simple",
    "lookback_window": 252,
    "rebalance_frequency": 21,
    "strategy_name": "all",
    "initial_capital": 1000000.0,
    "use_gpu": true,
    "admm": {
        "confidence_level": 0.95,
        "max_iter": 500
    },
    "monte_carlo": {
        "n_scenarios": 50000,
        "seed": 42
    },
    "factor_model": {
        "n_factors": 10
    },
    "use_factor_mc": true,
    "mean_variance": {
        "shrinkage_intensity": 0.5
    },
    "transaction_costs": {
        "cost_rate": 0.001,
        "min_trade_threshold": 0.005
    },
    "output_dir": "results/backtest_sp500_50",
    "verbose": false
}
