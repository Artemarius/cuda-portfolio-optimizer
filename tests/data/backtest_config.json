{
    "price_csv_path": "test_prices.csv",
    "tickers": ["AAPL", "MSFT", "GOOG"],
    "start_date": "2023-01-01",
    "end_date": "2023-12-31",
    "return_type": "simple",
    "lookback_window": 60,
    "rebalance_frequency": 21,
    "strategy_name": "EqualWeight",
    "initial_capital": 100000.0,
    "use_gpu": false,
    "admm": {
        "confidence_level": 0.95,
        "rho": 1.0,
        "max_iter": 200,
        "abs_tol": 1e-5,
        "rel_tol": 1e-3
    },
    "monte_carlo": {
        "n_scenarios": 5000,
        "seed": 42
    },
    "mean_variance": {
        "shrinkage_intensity": 0.1
    },
    "transaction_costs": {
        "cost_rate": 0.001,
        "min_trade_threshold": 0.001
    },
    "output_dir": "results/backtest",
    "verbose": false
}
